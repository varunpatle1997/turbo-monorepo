{
  "name": "vue-app-2",
  "version": "0.0.0",
  "dependencies": {
    "axios": "^1.7.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0"
  },
  "private": true,
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "check-types": "tsc --noEmit",
    "codeql-run-analysis": "codeql database create ./codeql-database --language=javascript-typescript --source-root=. --overwrite && codeql database analyze ./codeql-database codeql/javascript-queries:codeql-suites/javascript-code-scanning.qls --format=sarif-latest --output=./codeql-database/vue-app-2.sarif --sarif-category=vue-app-2 --threads=0 --sarif-add-baseline-file-info --verbose",
    "codeql-upload-result": "codeql github upload-results --sarif=./codeql-database/vue-app-2.sarif --github-url=https://github.com/ --repository=${GITHUB_REPOSITORY} --ref=${GITHUB_REF} --commit=${GITHUB_SHA}",
    "deploy": "swa deploy --config swa-cli.config.json --deployment-token=$DEPLOY_TOKEN_VUE_APP_2",
    "dev": "NODE_ENV=development vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "vite preview",
    "sonar": "sonar-scanner -Dsonar.projectKey=VUE-APP-2 -Dsonar.projectName=VUE-APP-2 -Dsonar.organization=divyanshu-startup -Dsonar.sources=./src -Dsonar.tests=./src/tests -Dsonar.test.inclusions=**/*.spec.ts -Dsonar.host.url=https://sonarcloud.io -Dsonar.token=$SONAR_VUE_APP_2",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run"
  },
  "type": "module"
}
