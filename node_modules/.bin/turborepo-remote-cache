#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules/turborepo-remote-cache/dist/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules/turborepo-remote-cache/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules/turborepo-remote-cache/dist/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules/turborepo-remote-cache/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/turborepo-remote-cache@2.2.8/node_modules:/home/varun_patle/webDev/deno-vue-turborepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../turborepo-remote-cache/dist/cli.js" "$@"
else
  exec node  "$basedir/../turborepo-remote-cache/dist/cli.js" "$@"
fi
