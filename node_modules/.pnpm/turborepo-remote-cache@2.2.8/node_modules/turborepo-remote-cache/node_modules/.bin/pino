#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pino@9.6.0/node_modules/pino/bin.js" "$@"
else
  exec node  "$basedir/../../../../../pino@9.6.0/node_modules/pino/bin.js" "$@"
fi
