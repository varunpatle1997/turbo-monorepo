{
  "name": "my-apps",
  "devDependencies": {
    "prettier": "^3.4.2",
    "syncpack": "^13.0.1",
    "turbo": "^2.4.1",
    "turborepo-remote-cache": "^2.2.8",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.2.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "cache-server": "turborepo-remote-cache",
    "sync:fix": "pnpm exec syncpack fix-mismatches",
    "sync:format": "pnpm exec syncpack format",
    "sync:list": "pnpm exec syncpack list",
    "sync:mismatches": "pnpm exec syncpack list-mismatches",
    "sync:update": "pnpm exec syncpack update"
  }
}
